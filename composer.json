{
    "name": "pollora/helper-overrider",
    "description": "Override some Laravel helpers",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Olivier Gorzalka",
            "email": "olivier@amphibee.fr",
            "role": "Developer"
        }
    ],
    "type": "composer-plugin",
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "require": {
        "php": "^8.2.0",
        "composer-plugin-api": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Pollora\\HelperOverrider\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "class": "Pollora\\HelperOverrider\\Plugin\\HelperLoader",
        "plugin-modifies-downloads": true
    }
}
